var nyc_events = [{"Address": "8-08 Queens Plaza South Entrance on, Queens County, NY 11101", "Artist": "Rick Wonder ", "ArtistImage": "https://photos.bandsintown.com/thumb/11559903.jpeg", "Coordinates": [40.7537922, -73.9494577], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Open Format Club And Radio Dj, Edm, House, Tech House, Open Format, Bass House", "Time": "2:00 PM", "Venue": "Profundo Day Club", "currentUrl": "https://www.bandsintown.com/e/102767580-rick-wonder-at-profundo-day-club?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "", "otherInfo": "No event info available"}, {"Address": "260 Meserole St, Brooklyn, NY 11206", "Artist": "Carry ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": [40.708425, -73.93818920000001], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "3:00 PM", "Venue": "3 Dollar Bill", "currentUrl": "https://www.bandsintown.com/e/1022583560-carry-at-3-dollar-bill?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "SUMMER SKOOL with SAUCY SANTANA"}, {"Address": "110-00 Rockaway Blvd, Jamaica, NY 11420", "Artist": "Crash The Party Band ", "ArtistImage": "https://photos.bandsintown.com/thumb/11591549.jpeg", "Coordinates": [40.6727098, -73.83279569999999], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "80s, Rock, Top 40 Hits, Music From 2000-2009- Duh., Hip Hop, Edm", "Time": "4:00 PM", "Venue": "Crash The Party Live at Resorts World Casino NYC", "currentUrl": "https://www.bandsintown.com/e/102784079-crash-the-party-band-at-resorts-world-casino-new-york-city?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "In early 2018, 24 year old Carlos Gabriel (Lead Singer) approached 107 Productions, asking what needed to be done to put together the next \"big band\". and their reply was simple; Assemble a band of young, energetic, talented, and reliable musicians. Design a production show that is unique to your band. Put a lot of thought into song choices and mashups, giving patrons an evening of music they can't hear any place else.After months of finding the right players and putting together the perfect show, Crash The Party launched in the spring of 2019 and hit the ground running. In a matter of weeks, we secured rotation in some of the country's finest venues, resulting in a nearly sold-out summer calendar in hot properties such as: Ballys, Billy Joe's Ribworks, Empire City, Hard Rock, Jenkinsons, King Neptunes, Oceans, Paradise Grill, Resorts World, Rivers Casino, Seacrets, The Borgata, The Marlin, The Nutty Irishman and Xfinity Live.", "otherInfo": "No event info available"}, {"Address": "605 3rd Ave, New York, NY 10158", "Artist": "DJ kdotlim ", "ArtistImage": "https://photos.bandsintown.com/thumb/9346899.jpeg", "Coordinates": [40.7490074, -73.9751509], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Funk, Edm, 10s, Reggeaton, Salsa, 70s, Afrobeat, Deep House, Dancehall, R&b, Top 40, Dance, 00s, 60s, New Wave, Soulection, Classic Rock, Pop, 80s, Indie, Nudisco, Rock, Open Format, Reggae, 90s, Soul, Alternative, Trap, Latin, House, Dembow, Country, Disco, Hip Hop, Soca", "Time": "5:00 PM", "Venue": "wagamama", "currentUrl": "https://www.bandsintown.com/e/102751791-dj-kdotlim-at-wagamama?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "kdotlim", "otherInfo": "No event info available"}, {"Address": "85 Ave A, New York, NY 10009", "Artist": "Gili Yalo ", "ArtistImage": "https://photos.bandsintown.com/thumb/11407682.jpeg", "Coordinates": [40.72525, -73.9844492], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Jazz, Ethio-funk, Fusion, New Soul", "Time": "5:00 PM", "Venue": "Gili Yalo", "currentUrl": "https://www.bandsintown.com/e/102672512-gili-yalo-at-drom?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Gili Yalo incorporates sounds from traditional Ethiopian music into a contemporary music production. The result is an exceptional, rich, vivid melody accompanied by Gili\u2019s unique voice with lyrics in both English and Amharic. Link to order the vinyl: goo.gl/EAnSht Link to download the album on iTunes: goo.gl/a9yt9K", "otherInfo": "No event info available"}, {"Address": "913 Ocean Ave, Asbury Park, NJ 07712", "Artist": "Tug of War Machine ", "ArtistImage": "https://photos.bandsintown.com/thumb/11590746.jpeg", "Coordinates": [40.2200031, -74.0008579], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "5:00 PM", "Venue": "Limp Bizkit & Spiritbox @ Stone Pony SummerStage ft. Tug of War Machine(Pre-Game Party) ", "currentUrl": "https://www.bandsintown.com/e/102783660-tug-of-war-machine-at-stone-pony?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "First show back (since pre pandemic) Headlining the Pre-game Party for\nLimp BIZKIT. Bring It on!"}, {"Address": "2811 Middle Country Rd, Lake Grove, NY 11755", "Artist": "Dudley Music ", "ArtistImage": "https://photos.bandsintown.com/thumb/8371487.jpeg", "Coordinates": [40.8602089, -73.1118631], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "R&b/soul, Rnb-soul, Jazz, R&b, Hip Hop", "Time": "5:30 PM", "Venue": "Village Idiot Pub Lake Grove", "currentUrl": "https://www.bandsintown.com/e/102723643-dudley-music-at-village-idiot-pub-lake-grove?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Dudley Music is a Long Island-based Hip Hop singer-songwriter, producer, and jazz musician, playing saxophone, keys, guitar, drums, flute, and clarinet. His debut EP, On My Way, was released in the Spring of 2016. With support from Talib Kweli, an extended version of the project was made available at Talib's online store, KweliClub.com.", "otherInfo": "No event info available"}, {"Address": "116 & Garden State Pkwy, Holmdel, NJ 07733", "Artist": "Dee Jay Silver ", "ArtistImage": "https://photos.bandsintown.com/thumb/11290902.jpeg", "Coordinates": [41.3044719, -72.9262301], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Country, Dance-pop", "Time": "6:00 PM", "Venue": "Back In The Saddle Tour", "currentUrl": "https://www.bandsintown.com/e/102611818-dee-jay-silver-at-pnc-bank-arts-center?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "What's up guys.. .welcome to my page hope to see you guys on the road soon!!!! Check out my website for all tour dates and information! www.deejaysilver.com", "otherInfo": "No event info available"}, {"Address": "134 Lower Sheep Pasture Rd, Setauket- East Setauket, NY 11733", "Artist": "Karen Bella ", "ArtistImage": "https://photos.bandsintown.com/thumb/11071143.jpeg", "Coordinates": [40.92000180000001, -73.10946109999999], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "St. Georges Golf and Country Club", "currentUrl": "https://www.bandsintown.com/e/102549216-karen-bella-at-st.-georges-golf-and-country-club?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "New York-based Karen Bella, is one of the hardest working singer-songwriters in the business, and after enduring a 2020 filled with set-backs, many may say she is also one of the most resilient. Pre-Pandemic, she performed eight to 10 shows a week in the tri-state area, playing such notable venues as the Rockwood Music Hall, The Bitter End, the Landmark Theater in Port Washington, Great South Bay Music Festival and the Philadelphia Music Festival. Despite her busy schedule, she still had time to write, record and release her debut album and perform high-profile shows that not only earned her a VIP audition for NBC\u2019s The Voice, she was named a Sam Ash Music Store \u201cSpotlight Artist,\u201d and she has been an official Fishman endorsed artist since 2019. In March of 2020, Bella was set to release her new, self-titled, six-song EP, produced by Josh Dion of Paris Monster, which she describes as \u201ccapturing her experiences and spirit,\u201d and one that was already being hailed as her strongest to date. Karen Bella premiered live at the Rockwood Music Hall in NYC on March 13, 2020, the night before the world stopped due to Covid-19. Following the performance, she suffered a broken wrist requiring surgery and several months of therapy, and then she contracted Covid-19. Deciding to put the album release on hold, Bella focused on getting healthy. Finally, now in 2021, after recovering successfully, Karen Bella, is being released to radio and getting the attention it deserves. Her self-titled project is a heartfelt record that transcends genres and encourages deep listening. The lyrics on Karen Bella, at times, feel light and free, then dive without apology into traumatic relationships,betrayal,andpaststruggleswithsubstance abuse.Thoughevery song has a different twist stylistically, the production team and musicians worked hard to craft a consistent, crisp, authentic, timeless, and even dreamy sound. \u201cI grew up listening to a multitude of genres. As a songwriter, there are many benefits to having genre versatility,\u201d said Bella. \u201cI get to share my love for Folk, Rock, R&B, Pop, and Country to its fullest. This record represents that. I want my fans to enjoy these little chapters of my life. My goal as a songwriter is to have a positive effect on the listener, because music has been a miraculous coping mechanism for me, helping me to fight the depression and anxiety that followed traumatic life events.\u201d Bella began studying music and acting in elementary school. She started book- ing for recording sessions at nine years old and was writing songs at age 12. After college, she pursued music full-throttle, releasing her first album, Ordinary Girl in 2013 and performing in legendary venues around NYC. During the Pandemic, Bella has been featured on News 12 Long Island and in the Long Island music publication, Good Times Magazine, speaking about the struggles of being an artist at this time \u2013 including contracting Covid herself \u2013 and inspiring musicians to continue creating, no matter what life throws at you. Facebook/Instagram: @karenbellamusic Twitter: @karen_bella_", "otherInfo": "No event info available"}, {"Address": "239 E 53rd St, New York, NY 10022", "Artist": "Ryoju Fukushiro ", "ArtistImage": "https://photos.bandsintown.com/thumb/11479707.jpeg", "Coordinates": [40.7572081, -73.96778599999999], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Classical, Smooth Jazz, Jazz, Funk, Lounge, Pop, Free Jazz", "Time": "6:00 PM", "Venue": "Jazz Juvenilia", "currentUrl": "https://www.bandsintown.com/e/102774785-ryoju-fukushiro-at-tomi-jazz?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Ryoju Fukushiro is a Jazz saxophone anomaly, recognized as delivering a vibrant timbre based on his distinct musicality, which relieves and amuses a wide range of audiences. He was born into a musical family in Kyoto, Japan in April 1986. Gaining a skill of a lyrical tone reminiscent of Stan Getz, Ryoju had been known as one of potential leading Jazz saxophonists. As he accumulating approvals, he won the Grand Prix in \u201cThe Third Kobe Next Jazz Competition\u2019\u2019 in 2009. His musical peculiarities were valued by phenomenal jazz critics such as Masahisa Segawa and Birdman Koda, he made an appearance in the Jazz critique Magazine \u201cJazz Hihyou\u2019\u2019 and \u201cJazz World\u201d as a young energetic saxophonist. After that, his original CD \u2018\u2019Bamboo Grove\u2019\u2019 was published, the original tune ''Bamboo Grove'' was aired on radio for an electricity power supply company, YAMABISHI's commercial song. In the summer of 2012, Ryoju was selected as a member of Korea Japan Jazz All Stars touring band formed by 4 splendid Korean and Japanese musicians, guitar-Kang Woong, trumpet-Akira Tanidono, piano-Song Yong Joo, bass-Park Jin Kyo, drum-Kim Min Chan. The shows had succeeded in every important venue at the Bear Hall, Evans Club, All That Jazz, ChunNyunDongAnDo, and Palm in Seoul, Korea. Accordingly, in March 2013, Ryoju visited San Francisco to participate in the Jazz competition \u2018\u2019Jazz Search West 2013\u2019\u2019. He won First Place the following month at the finals. As the prize, Ryoju received full tuition to the summer camp \u2018\u2019Jazz Camp West 2013\u2019\u2019. In the Camp, he performed in one of the faculty concerts as a guest saxophonist with Elio Villafranca, Allison Miller, Ratzo B. Harris. In November 2013, Ryoju moved to the New York City vicinity to discipline himself, and study Jazz as a music based on American culture. In March 2015, Ryoju was accepted to the Manhattan School of Music. And also, he was transferred to CUNY Queens College, Aaron Copland School of Music. Then he graduated with the degree of Master of Music in Jazz Study in 2018. Ryoju was invited to join \u201cGeorge Gershwin International Music Competition2017 in New York\u201d as a Classical-Jazz saxophonist, and won third prize in Winds and Brass category in the competition. In March 2018, as a composer, sound designer, music director, Ryoju was selected for composing and designing an original main theme song for Japanese Kids\u2019 school in New Jersey \u201cThe Kids International Weekend School, Inc.\u201d. He contributed his all musical and engineering abilities into the song called, \u201cImadokino-Aiueo\u201d. He has been performing at wide variety music venues and restaurants in the city, such as Tomi Jazz, Brooklyn Bowl, Gramercy Park Hotel, Shrine, Silvana, The Bitter End, and Giando on the Water, ZAVO restaurants. Ryoju has become an official endorser of the saxophone maker, Forestone Japan, and has been playing the Forestone Saxophone. Equipment: FORESTONE Tenor RX GL", "otherInfo": "[Jazz Juvenilia]\nE.Piano - Andi Wang, \nE.Bass - Kenji Tokunaga,\nDrums - Yuko Togami, \nSaxophone - Ryoju Fukushiro"}, {"Address": "909 Ocean Ave, Asbury Park, NJ 07712", "Artist": "Limp Bizkit ", "ArtistImage": "https://photos.bandsintown.com/thumb/6825760.jpeg", "Coordinates": [40.219798, -74.001015], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Alternative Hip Hop, Nu Metal, Rock Rap, Rapcore", "Time": "6:00 PM", "Venue": "The Limited Last Minute Post Pandemic Popup Party ", "currentUrl": "https://www.bandsintown.com/e/102704010-limp-bizkit-at-stone-pony-summer-stage?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "*THIS IS THE OFFICIAL LIMP BIZKIT FACEBOOK PAGE* The original line-up of Fred Durst, Wes Borland, Sam Rivers, John Otto, and DJ Lethal are back from an eight year hiatus to bring their world back to ours. Fred Durst and Wes Borland said in a joint statement: \"We decided we were more disgusted and bored with the state of heavy popular music than we were with each other. Regardless of where our separate paths have taken us, we recognize there is a powerful and unique energy with this particular group of people we have not found anywhere else. This is why Limp Bizkit is back.\" The guys will kick off a world tour in the Spring on the overseas festival circuit with headline shows sprinkled in throughout Eastern Europe and Europe, selected dates include, in late May, Russia, Ukraine, and the Baltic countries where Limp Bizkit has never played before despite huge demand, along with major festivals like Rock Am Ring and Rock Im Park in Germany, more dates to be announced shortly. A new album, which would be the original group\u2019s first full-length effort since 2000, is also planned. Limp Bizkit\u2019s first three albums have sold over 20 million copies in the U.S. alone, and another 13 million in the rest of the world.", "otherInfo": "No event info available"}, {"Address": "141 Prospect Park West, Brooklyn, NY 11215", "Artist": "Buffy Sainte-Marie ", "ArtistImage": "https://photos.bandsintown.com/thumb/6304611.jpeg", "Coordinates": [40.6633884, -73.9766537], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Folk", "Time": "6:00 PM", "Venue": "BRIC Celebrate Brooklyn! Festival", "currentUrl": "https://www.bandsintown.com/e/102660905-buffy-sainte-marie-at-prospect-park-bandshell?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Academy Award winner Buffy Sainte-Marie\u2019s audacious attitude to life on and off the stage has inspired people around the world for over five decades.", "otherInfo": "Free Admission with RSVP: https://www.universe.com/events/buffy-sainte-marie-naia-kete-tickets-VD2WZY"}, {"Address": "173 Morgan Ave, Brooklyn, NY 11206, USA, New York City, NY", "Artist": "Skech185 ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": [40.7101142, -73.9336187], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Arrogant Swine", "currentUrl": "https://www.bandsintown.com/e/1022482134-skech185-at-arrogant-swine?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "173 Morgan Ave, Brooklyn, NY 11206, USA, New York City, NY", "Artist": "Duncecap ", "ArtistImage": "https://photos.bandsintown.com/thumb/8745176.jpeg", "Coordinates": [40.7101142, -73.9336187], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Arrogant Swine", "currentUrl": "https://www.bandsintown.com/e/1022481214-duncecap-at-arrogant-swine?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Duncecap makes hip hop music about anything.", "otherInfo": "No event info available"}, {"Address": "599 Johnson Ave #1, Brooklyn, NY 11237, USA, New York City, NY", "Artist": "MIKE SERVITO ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": ["", ""], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Techno, Electronic", "Time": "6:00 PM", "Venue": "Elsewhere - Rooftop", "currentUrl": "https://www.bandsintown.com/e/1022143527-mike-servito-at-elsewhere-rooftop?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "599 Johnson Ave #1, Brooklyn, NY 11237, USA, New York City, NY", "Artist": "Jason Kendig ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": ["", ""], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "R&b/soul, Rnb-soul, Techno, Electro Pop, Pop, Blues, Soul, Electronic, Electro House, Deep House, R&b, Disco, Tech House", "Time": "6:00 PM", "Venue": "Elsewhere - Rooftop", "currentUrl": "https://www.bandsintown.com/e/1022143124-jason-kendig-at-elsewhere-rooftop?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "40 Millhurst Rd, Manalapan Township, NJ 07726", "Artist": "Oliver Dagum ", "ArtistImage": "https://photos.bandsintown.com/thumb/9340454.jpeg", "Coordinates": [40.264594, -74.337398], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Easy Listening, Acoustic, Pop, Soft Rock", "Time": "6:00 PM", "Venue": "Battleground Country Club", "currentUrl": "https://www.bandsintown.com/e/102524028-oliver-dagum-at-battleground-country-club?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Oliver Dagum is an accomplished singer-songwriter from Mount Holly NJ and a veteran of the United States Air Force. In 2008, Oliver was 1 of 10 male performers chosen to be part of the Air Force Entertainment \"Tops in Blue\", one of the oldest and most widely traveled entertainment groups of its kind in the USAF. He performed over 130+ shows set out to entertain and provide morale to military troops and their families at Air Force bases around the world. After the tour, Oliver moved to McGuire Air Force Base New Jersey. He started performing in open mic nights and various places from New York City, Philadelphia and Atlantic City. To include the world-famous Coney Island Boardwalk, Foley Square, Atlantic City, and New Jersey Boardwalks. He separated from the military to pursue his music career, in 2015. In that same year, he was 1 of the 26 selected by (MUNY)Music Under New York out of thousands that sent in their application for a spot to perform in the subways in New York City as part of MTA(Metropolitan Transit Authority) Arts and Design. Only select 20+ musicians are selected annually. A couple of months later, he was also selected for New Jersey Transits \"Music in Motion\". Oliver is a graduate of Berklee College of Music, where he completed his Professional Certificate in Music Business. He took courses in Music Publishing, Artist Management, Music Licensing, Music Marketing, and Music Supervision. He was then able to produce and publish his own music independently. He also finished Coursera's Modern Musician's Courses that included Songwriting, Musicianship and Music Production. Oliver's music is composed of acoustic, easy-listening music. He stated, \"I love to remake songs to a nice, relaxing feel to it. It is akin to driving music.\" His original songs are now available on Itunes, Amazon MP3 and other digital stores.", "otherInfo": "Performing at Battleground Country club members only from 6-10 PM. Don\u2019t forget to reserve your seats :)"}, {"Address": "130 Hardscrabble Rd, North Salem, NY 10560", "Artist": "Raquel de Souza ", "ArtistImage": "https://photos.bandsintown.com/thumb/9190773.jpeg", "Coordinates": [41.34991489999999, -73.6350863], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Rock, Country", "Time": "6:00 PM", "Venue": "HARVEST MOON FARM & ORCHARD: (Raquel & The Wildflowers)", "currentUrl": "https://www.bandsintown.com/e/102525274-raquel-de-souza-at-harvest-moon-farm-and-orchard?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "531 Cookman Ave, Asbury Park, NJ 07712", "Artist": "Walter Lee ", "ArtistImage": "https://photos.bandsintown.com/thumb/11105192.jpeg", "Coordinates": [40.2162849, -74.0100806], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Acoustic, Reggae, Blues, Jazz, Funk, Rock", "Time": "6:00 PM", "Venue": "Walter Lee at Asbury Ale House 6pm", "currentUrl": "https://www.bandsintown.com/e/102693729-walter-lee-at-asbury-ale-house?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Walter Lee is a singing acoustic loop artist from Stroudsburg, PA. He has performed up and down the New York Metropolitan Area. Walter plays covers from popular artists like Sublime, Incubus, Jack Johnson, John Mayer, Blues Traveler, Jimi Hendrix, The Beatles, and more. His originals reflect a similar style to the covers he performs with an ethereal atmosphere. Walter Lee is also known for discovering and leading the band Tracksuit Panda. He also takes part in South of the Peak, The Crowning, and From The Dark River. Currently, Walter is writing and recording a new album set to release sometime in 2021. You can find his music on social media. \u201cI enjoy performing and meeting people from all over. I\u2019m happy to play in a crowd of 500 or a crowd of 10.\u201d", "otherInfo": "No event info available"}, {"Address": "682 County Rd 1, Pine Island, NY 10969", "Artist": "Whiskey Crossing ", "ArtistImage": "https://photos.bandsintown.com/thumb/11511032.jpeg", "Coordinates": [41.2975225, -74.4659805], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Whiskey Crossing Duo at Pine Island Tap House", "currentUrl": "https://www.bandsintown.com/e/102742896-whiskey-crossing-at-pine-island-brewing-company?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Whiskey Crossing is a rising Country Blues/Country Rock duo that surely brings the heat and excitement into the room. They most recently played a sold out show to over 900 people at The Chance Theatre in Poughkeepsie, NY opening for Rodney Atkins. They are climbing their way to the top and fast, sharing the stage with some of Country Music's biggest stars like Travis Tritt and Rodney Atkins. In 2017 they were named best country act of the year by New York Associated Press Music Critic and world-renown photographer Lisa Piernot. In 2018 they were nominated for duo of the year & song of the year on the Nashville Universe Awards as well as being listed in the Top 10 on WBCW Florida Country Radio. In 2019 they were featured in Reverbnation's Top 6 \"Chart Topping New York City Artists\" out of hundreds of NYC artists. Their original music is played in six different states on FM stations and has been featured on many TV shows such as The Ginger New York Show, Keeping Country Strong, Southern Country Radio, Country 107.3 WRWD, among many other TV and Radio stations. Whiskey Crossing has their back up band that adds to this amazing act. You can see Whiskey Crossing as a duo or a band playing weekly in local venues spending 4 to 5 nights a week with their audience keeping them dancing the night away and is known to give a one-of-a-kind, high energy, interactive show that keeps you singing all night long!!! Whiskey Crossing has performed all over the Nation sharing the stage with amazing National Acts, such as Travis Tritt, Rodney Atkins, Lee DeWyze (American Idol Winner Season 9), Lee Rocker (Stray Cats), Foghat, Artimus Pyle Band, Outlaws, The Weight Band, Hannah Wicklund & The Stepping Stones, Poco, Antigone Rising and Soulsytem (feat. Rob Affuso, drummer from SKID ROW). Whiskey Crossing can also be seen in Nashville Tennessee performing, which is also where they write most of their music . Signed it to Whiskey Hill Records in 2017, this duo is simply unstoppable. Tina Marie and her powerhouse vocals with Andrew Gallagher and his amazing rhythm guitar, is a duo known for their show-stopping harmonies; they will never disappoint. When these two are not playing music, they are caring for their rescue farm in the farmlands of New Jersey. You definitely want to catch a show!", "otherInfo": "For more info visit www.whiskeycrossing.com"}, {"Address": "703 16th Ave, Belmar, NJ 07719", "Artist": "Colossal Street Jam ", "ArtistImage": "https://photos.bandsintown.com/thumb/11232610.jpeg", "Coordinates": [40.1743861, -74.0283139], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Rock, Blues", "Time": "6:00 PM", "Venue": "CSJ & RESX", "currentUrl": "https://www.bandsintown.com/e/102575168-colossal-street-jam-at-bar-anticipation?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Mix the gritty, soulful vocals of Gene Potts with the guitar fire of Sal Marra, the pulsing bass of Anthony Flora, the war-hammer drums of Dave Halpern and the one man festival that is Eric Safka on keys, and you get Colossal Street Jam. Colossal Street Jam is a 5 piece band based out of Red Bank, NJ that plays a 70\u2019s style brand of infectious and melodic rock and roll. Colossal Street Jam\u2019s November 2016 release \u201cLiving Free\u2019 has garnered critical acclaim and has been featured in many music magazines worldwide and continues to get extensive radio play both locally and globally. In January of 2018, \u201cLiving Free\u201d won the Australian Indie Album of the Year award for internet radio and is currently on the charts in Australia with their new single \u201cJust Take Hold\u201d. In June of 2018 there will be a new release by Colossal Street Jam but until that time they will continue to put out one single a month to keep the buzz on this band at a fevered pitch. CSJ has shared the stage with many great artists throughout their career including: Robbie Krieger, Kings X, Blackberry Smoke, The Drive-by Truckers, The Hold Steady, Gov\u2019t Mule, Johnny Winter, Sebastian Bach, Bruce Dickinson, Screamin Cheetah Wheelies, Ritchie Blackmore, Steve Morse, Zebra, Deep Purple, Leslie West, Clarence Clemons and many more. Colossal Street Jam is: Gene Potts \u2013 Vocals Sal Marra \u2013 Guitar, Vocals & Fender Rhodes Tony Flora \u2013 Bass Dave Halpern \u2013 Drums & All Percussion Eric Safka \u2013 Hammond B3, Piano, Fender Rhodes", "otherInfo": "Can't stop - Won't Stop!\nColossal Street Jam and Resurrextion join up and play Bar A once again on August 6th. All three of our previous shows sold out quickly so get yours ASAP!\nThis is an outside show and will hold 400 people safely."}, {"Address": "106-01 Shore Front Pkwy, Queens County, NY 11694", "Artist": "Stell and Snuggs ", "ArtistImage": "https://photos.bandsintown.com/thumb/8047124.jpeg", "Coordinates": [40.5805679, -73.8260751], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "International, Folk, World, Experimental, Acoustic", "Time": "6:00 PM", "Venue": "Stell & Snuggs @ Caracas", "currentUrl": "https://www.bandsintown.com/e/102785749-stell-and-snuggs-at-caracas-arepa-bar-(summer-only)?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "\u201cIt has meant a strengthening of our souls\u2026and replacing substance with sustenance.\u201d -The New York Times One sentence printed has revealed a new dimension into the lives of a husband/wife band\u2019s reason for being. Coincidence and serendipity are the two words that explain Stell & Snuggs since its inception. How music can ease the soul and transport us- this sums up the reflective process the two wrapped themselves in as they traded in their separate music careers, and boldly joined, moving on a sailboat and taking their music to oceans and islands. Jubilant, happy, promising, and building music that paints their portrait, sharing a love of freedom, dreaming up sounds they could not produce on their own. Stell & Snuggs are Christel Astin (flute,uke,vocals) and Jarad Astin (accordion, stompbox, vocals). This once Brooklyn, NY duo are packed with immediate fun moments, their seasoned musicianship is incredible especially considering the variety of instruments implemented. Their July 2015 release, Vela guides you through an enticing journey of highs and lows, vocals and instrumentals that bind together, and enclose musical elements of Colombian Cumbia, Gypsy, Country, and Brazilian Choro. While sitting one night at one of their performances, one listener wrote to them in a fan email, \u201cThank you for the beautiful music that you brought to us all last night, you were both absolutely enchanting.\u201d And Rob Wasserman of the Colorado Daily said this of flutist, Christel Astin, \u201cHer runs and articulations are speedy, but nonetheless possessed of a plaintive soulfulness and verve that reminds the listener of an ecstatic nightingale.\u201d Their mode of touring transport, a 43 ft C&C Landfall sloop (sailboat) has given them the opportunity of perform in a wide variety of venues, from the creme-de-la-creme to their favorite local hangouts. Here\u2019s a short list of some of their clients: Coconut World Cup SUP Festival (ST.CROIX) Antigua Classic Yacht Regatta (ANTIGUA) West Indies Regatta (ST.BARTHS) Caribbean Arts Festival (BVIs) Saint Croix Jump Up Festival (ST.CROIX) The Blue Note (NYC) Denver People\u2019s Music Festival (CO) Webster Hall (NYC) Kupferberg Center of the Arts (NYC) NYC Parks Dept \u201cMusic in the Park\u201d Summer series The Marlo Thomas Show (NYC) Huntington Summer Festival (NY) the Bluebird Theater (CO) Two Boots of Brooklyn (NYC) In addition to these venues, they are part owners and producers for Cuebro Music Productions and have produced music for film in a variety of settings. Their client list includes Corona and San Miguel beers, Channel One Productions, Neutralizer Shoes, the film \u201cUnlimited,\u201d Netflix Original film, \u201cWhat Happened to Nina Simone,\u201d and many more. It\u2019s exemplar, and escalating. This is the aim of Stell &Snuggs, sailing with little impact, toting their music from port to port around the world, with a tune to share. An acoustic gypsy soul band tour by sail Christel Rice: flutes, ukulele, vocals Jarad Astin: accordion, vocals, stompbox", "otherInfo": "No event info available"}, {"Address": "909 Ocean Ave, Asbury Park, NJ 07712", "Artist": "SpiritBox ", "ArtistImage": "https://photos.bandsintown.com/thumb/11434233.jpeg", "Coordinates": [40.219798, -74.001015], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Metalcore, Metal", "Time": "6:00 PM", "Venue": "Stone Pony Summer Stage", "currentUrl": "https://www.bandsintown.com/e/102699651-spiritbox-at-stone-pony-summer-stage?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "The husband and wife duo Courtney LaPlante and Michael Stringer made their debut with a self-released EP in October 2017, quickly building a cult following. In 2018 the duo found their musical counterpart in bass player Bill Crook and partnered with Pale Chord Music to continue to create and release music in single and EP formats. Spiritbox have quickly become hotly tipped as one of the most exciting, creative and unique new forces to erupt from the world of heavy music. Pushing boundaries both sonically and visually with their beautifully haunting music videos that perfectly mach the complex, heavy instrumentation combined with the juxtaposition of the ethereal, velvet textures of Courtney's singing voice and the visceral, rageful power of her screams. Drawing their name from a device that some believe is capable of communing with the dead, the band have a paranormal undertone that runs through everything they do, using EVP (electronic voice phenomena) recordings both at their live shows and within their tracks. A quick look at music videos for fan favorites such as Belcarra, Blessed Be and Rule of Nines and it's clear to see that the band are no strangers to the macabre or paranormal. Having toured the world many times over with their previous bands - metal outfit iwrestledabearonce (Courtney and Mike) and pop punk band Living With Lions (Bill) - Spiritbox absorbed the highs and lows of living as working musicians and decided to try a different and more sustainable approach when it came to releasing their music. They took the concept of supply and demand and applied it to their release model, focusing on making fans across the globe by using the social media tools available to them, releasing shorter bodies of music and removing touring from the plan until a demand was built. In doing so they quickly garnered a cult-like online following which has lead to over 10,000,000 streams across all platforms and sell outs on every physical release and most merch items they've released to date. In Feb of 2020 Spiritbox jumped on a plane and finally went on tour in support of After The Burial across Europe, which was unfortunately cut short due to the Covid-19 Pandemic. Despite having to return to Canada early, Spiritbox have remained busy - releasing their latest single Holy Roller which debuted number 25 on the Billboard Hot Hard Rock Songs chart and debuted on Sirius XM Liquid Metal at number 9, climbing to number 1 for 7 consecutive weeks on the Devils Dozen countdown. Shooting it's accompanying video, launching their new Patreon and working on their debut LP expected out in 2021.", "otherInfo": "No event info available"}, {"Address": "2 Old Tomahawk St, Yorktown Heights, NY 10598", "Artist": "Dan Zlotnick ", "ArtistImage": "https://photos.bandsintown.com/thumb/11174517.jpeg", "Coordinates": [41.31531409999999, -73.7483373], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Blues & Rock, Folk, Folk Rock, Americana, Classic And Modern Rock, Acoustic Rock", "Time": "6:00 PM", "Venue": "Barnwood Grill Restaurant", "currentUrl": "https://www.bandsintown.com/e/102791937-dan-zlotnick-at-barnwood-grill-restaurant?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Dan Zlotnick of Westchester County, NY brings a wide range of influence to his heartfelt Americana style. Listeners of all ages love watching Dan pick and strum his vintage acoustic guitar as he tells stories with his rich voice and thoughtful lyrics. He also rips through guitar leads in his more blues-rockin\u2019 tunes, paying homage to his classic rock heroes. He has rocked the stages of the Paramount Hudson Valley, Pleasantville Music Festival, Tarrytown Music Hall, and Garcia\u2019s at The Capitol Theatre. Dan has also opened multiple shows for Grammy winner Marc Cohn, Top-40 Country Artist Jessica Lynn, and Americana's favorite father, Jim Avett. No matter the venue, audience members leave Dan\u2019s shows feeling like they have a new friend. For more information, visit www.DanZlotnick.com and follow Dan on social media.", "otherInfo": "6:00-9:00 pm - outdoors & weather permitting"}, {"Address": "323 Main St, Farmingdale, NY 11735", "Artist": "Live Vinyl ", "ArtistImage": "https://photos.bandsintown.com/thumb/11577617.jpeg", "Coordinates": [40.7318047, -73.4454829], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Latin, Rock, Pop, Indie Dance", "Time": "6:00 PM", "Venue": "Live Vinyl Outside Rock Party at The Nutty Irishman", "currentUrl": "https://www.bandsintown.com/e/102774624-live-vinyl-at-nutty-irishman?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Live Vinyl is a Rock, Pop, Live Dance band from New York. Started in 2017 to create a high energy band that is catchy and memorable. Live Vinyl has performed live for three years in NYC and Long Island music circuit. The band found it's own identity now finishing our first EP \"Living in The 2020's\". Live Vinyl features the amazing voices of Grace Nazario and David Karp. \"Get Out Party Today\", and \"Rock you through the Night\" are now available through all media outlets. Our new release \"faith or Hate\" is scheduled February 27th 2021.", "otherInfo": "Live Vinyl is firing on all cylinders as we bring our Dance Rock party to Farmingdale for our first outside @ The Nutty Irishman. \nAll of our shows bring new songs with Classic Rock, Dance, Pop, Funk and more from the 60's to the 2000's. Great time is had for all who attend, Free Live Music, we will see you there, CHEERS!"}, {"Address": "130 Hardscrabble Rd, North Salem, NY 10560", "Artist": "Raquel & the Wildflowers ", "ArtistImage": "https://photos.bandsintown.com/thumb/11385367.jpeg", "Coordinates": [41.34991489999999, -73.6350863], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Rock, Pop, Country", "Time": "6:00 PM", "Venue": "HARVEST MOON FARM & ORCHARD", "currentUrl": "https://www.bandsintown.com/e/102525245-raquel-and-the-wildflowers-at-harvest-moon-farm-and-orchard?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "599 Johnson Ave, Brooklyn, NY 11237, USA, New York City, NY", "Artist": "Mima Good ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": [40.7096703, -73.9232787], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Elsewhere - Zone One", "currentUrl": "https://www.bandsintown.com/e/1022560753-mima-good-at-elsewhere-zone-one?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "173 Morgan Ave, Brooklyn, NY 11206, USA, New York City, NY", "Artist": "Hijinx ", "ArtistImage": "https://photos.bandsintown.com/thumb/6291025.jpeg", "Coordinates": [40.7101142, -73.9336187], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Punk, Punk Rock And Roll", "Time": "6:00 PM", "Venue": "Arrogant Swine", "currentUrl": "https://www.bandsintown.com/e/1022481630-hijinx-at-arrogant-swine?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Contact Hijinx010@hotmail.com for Booking. http://hijinx.bigcartel.com/ http://eastgrand.bandcamp.com/", "otherInfo": "No event info available"}, {"Address": "173 Morgan Ave, Brooklyn, NY 11206, USA, New York City, NY", "Artist": "Nosaj ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": [40.7101142, -73.9336187], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Arrogant Swine", "currentUrl": "https://www.bandsintown.com/e/1022481384-nosaj-at-arrogant-swine?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "327 Franklin Ave J, Wyckoff, NJ 07481", "Artist": "Plane Station ", "ArtistImage": "https://photos.bandsintown.com/thumb/11280142.jpeg", "Coordinates": [41.0117993, -74.1677832], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "Plane Station at Blue Moon Mexican Cafe", "currentUrl": "https://www.bandsintown.com/e/102605936-plane-station-at-blue-moon-mexican-cafe?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Plane Station is a sibling singer/songwriter duo who's indie folk sound is driven by harmony and a devotion to honest lyricism. Nick and Brianna Chiapparino grew up singing together in the suburbs of Westchester, New York. The duo has fronted many bands spanning a wide variety of genres but for the first time, with Plane Station, are settling into the music they feel most at home with. Honing their craft through years of live performance, Nick and Brianna build their sound on an ability to write collaboratively and harmonize with one another that is, at this point, second nature. Their first album, The Other Side, finds the duo exploring themes of love, change and loss in a stripped down, predominantly acoustic setting. However, Plane Station is not averse to the grandiose and often stretch their acoustic sound into soaring harmonies and swelling instrumentation. This is evident not only in their sprawling live shows, which often feature drummer and producer Jesse Melito, but their two latest singles, Alcohol and Francis. Both of which tease Nick and Brianna\u2019s growth towards a more expansive sonic landscape on their upcoming LP, 7th Street.", "otherInfo": "No event info available"}, {"Address": "4 J F Kennedy Dr, Milltown, NJ 08850", "Artist": "ScreenAge ", "ArtistImage": "https://photos.bandsintown.com/thumb/11548623.jpeg", "Coordinates": [40.4508717, -74.4352967], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:00 PM", "Venue": "ScreenAge at the American Legion", "currentUrl": "https://www.bandsintown.com/e/102761276-screenage-at-american-legion?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "71 Merrick Rd, Amityville, NY 11701", "Artist": "Chris Milo ", "ArtistImage": "https://photos.bandsintown.com/thumb/11472554.jpeg", "Coordinates": [40.6712769, -73.42012179999999], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Pop, Rock", "Time": "6:30 PM", "Venue": "Chris Milo live at Sophia Italian Bistro", "currentUrl": "https://www.bandsintown.com/e/102631399-chris-milo-at-sophia?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "17-year-old singer/songwriter Chris Milo is a professional recording artist, multi-instrumentalist, actor, and dancer from Long Island, New York. Of Cuban/Puerto Rican and Italian descent, Chris has performed across New York City and Long Island, including appearances at Pianos, The Bitter End, The Marriot Marquis, The Roosevelt Hotel, and My Father\u2019s Place. Chris\u2019s love for music started at a young age, drawing influences from early soul/R&B artists such as Otis Redding, Nina Simone, Joe Bonamassa, as well as the pop/rock of Ritchie Valens, Del Shannon, Dion, Bobby Darin, and Simon & Garfunkel. Contemporary influences include Charlie Puth, Ed Sheeran, Shawn Mendes, Camila Cabello, Justin Bieber, and others. In addition to singing, Chris is also an accomplished dancer, having appeared at numerous, international Latin ballroom competitions. He has also appeared in independent films as well as performed on numerous main stage theater productions in the New York area. Chris\u2019s debut original single, \u201cPoisoned Love,\u201d along with a Spanish version entitled \u201cAmor Venenoso,\u201d were recorded at Richie Cannata's Cove City Sound Studios in Glen Cove, New York. Both recordings were written, produced, and arranged by Chris in both English and Spanish. They were available for preorder sale on major digital stores on January 8th, 2021, and officially released on January 15th. Additional songs were also recorded which will collectively comprise Chris\u2019s forthcoming debut EP.", "otherInfo": "Come see nationally-charted recording artist Chris Milo live at Sophia Italian Bistro in Amityville, NY on Friday evening, August 6th from 6:30-10PM and see for yourself what all the recent buzz is about!\n\nEnjoy some of the most delicious Italian dishes around as you relax and watch one of the most dynamic and exciting local performers in years entertain you for hours!\n\nChris Milo is a seasoned performer, having appeared on the Main Stage at NAMM as well as renowned venues including My Father's Place, Bitter End & Marriott Marquis. His debut \"Poisoned Love\" has been making waves on major radio stations across the country; now standing at #2 on New Music Weekly Hot 100 charts. He has also stood strongly for many weeks at #1 locally/regionally & #5 nationally/globally on ReverbNation's Pop charts.\n\nExperience the buzz yourself as Chris dazzles you with a mix of original songs, current Top 40 hits, & vintage classics dating back to the 1950's.\n\nPerformance time:\n6:30-10:00 PM\nReserve your table by calling Sophia Italian Bistro at:\n(631) 598-1150\nhttp://www.sophiabistro.com/\nFor more, please visit: https://bio.to/chrismilo\nAddress:\n71 Merrick Rd\nAmityville, NY 11701\n\nAbout Sophia Italian Bistro:\nSophia Bistro is an Italian Bistro-style restaurant, located in Amityville, New York. Sophia Bistro specializes in Italian cuisine, seafood, chops and pasta. We are so happy to welcome you Back for Outdoor Dining or Indoor Dining!"}, {"Address": "61 Washington St, Bloomfield, NJ 07003", "Artist": "Carla Virola ", "ArtistImage": "https://photos.bandsintown.com/thumb/11493671.jpeg", "Coordinates": [40.7935012, -74.1986534], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Dance Pop", "Time": "6:30 PM", "Venue": "Jerry Tolve Live! With Special Guest - Carla V @Bucco's of Bloomfield, NJ !", "currentUrl": "https://www.bandsintown.com/e/102794296-carla-virola-at-bucco-restaurant-bloomfield?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Carla hails from NYC's East Harlem, the second child born to Puerto Rican Parents. Her varied musical style comes from musical influences that include salsa, merengue, club/dance and rock music. Carla is a graduate of he Laguardia Highschool of the Arts as a vocal major. She went on to study acting and musical theatre at the American Musical and Dramatic Academy, inspiring her to pursue dramatic roles in theater, film and television. She is currently developing a web based webisodes project for 2016, to air on YouTube... Exciting times are ahead as Carla's vision to increase her artistic presence globally continues to gain momentum! Follow Carla on Twitter, Instagram and LinkedIn in addition to Facebook... Join Carla's Fan Club here by following her... Carla thanks each and every person who has supported her aspirations in the \"biz\" ...", "otherInfo": "Live Music and Entertainment by Jerry Tolve and Carla V every Friday @6:30pm at Bucco's of Bloomfield, NJ..!!"}, {"Address": "1 Stonehill Dr, Sussex County, NJ 07462", "Artist": "Annie Mash ", "ArtistImage": "https://photos.bandsintown.com/thumb/11590169.jpeg", "Coordinates": [41.1855419, -74.518886], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:30 PM", "Venue": "Annie Mash Duo at Kites ", "currentUrl": "https://www.bandsintown.com/e/102782960-annie-mash-at-minerals-resort-and-spa?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "Upstairs in Kites Restaurant @ Minerals"}, {"Address": "5-25 46th Ave, Queens County, NY 11101", "Artist": "SPACE CADE7S ", "ArtistImage": "https://photos.bandsintown.com/thumb/11170588.jpeg", "Coordinates": [40.7474559, -73.95417239999999], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Soul, Hip Hop, R&b, Rap", "Time": "6:30 PM", "Venue": "Culture Lab LIC", "currentUrl": "https://www.bandsintown.com/e/102594881-space-cade7s-at-the-plaxall-gallery?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "East Coast Hip-Hop/Soul Collective - WWW.SPACECADE7S.COM", "otherInfo": "No event info available"}, {"Address": "115 Macdougal St, New York, NY 10012", "Artist": "Bryan Stephens ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": [40.7301811, -74.0007016], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "6:30 PM", "Venue": "Cafe Wha", "currentUrl": "https://www.bandsintown.com/e/1022648887-bryan-stephens-at-cafe-wha?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "260 Meserole St, Brooklyn, NY 11206", "Artist": "In Living Color ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": [40.708425, -73.93818920000001], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "3 Dollar Bill", "currentUrl": "https://www.bandsintown.com/e/1022575324-in-living-color-at-3-dollar-bill?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "116 & Garden State Pkwy, Holmdel, NJ 07733", "Artist": "Jason Aldean ", "ArtistImage": "https://photos.bandsintown.com/thumb/9367360.jpeg", "Coordinates": [41.3044719, -72.9262301], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Country", "Time": "7:00 PM", "Venue": "Jason Aldean in Holmdel, NJ", "currentUrl": "https://www.bandsintown.com/e/102609801-jason-aldean-at-pnc-bank-arts-center?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "After almost 15 years at the top of his format, reigning ACM \"Artist of the Decade\" Jason Aldean has seen trends come and go. Hell, he helped bring a few out of the shadows, like a Country-music Columbus making the mainstream\u2019s first contact with hard rock (\u201cHicktown\u201d), hip-hop (\u201cDirt Road Anthem\u201d) and R&B (\u201cBurnin\u2019 It Down\u201d). For 9, Aldean's ninth studio project, he offers 16 interwoven tracks in an era of stand-alone singles and superstar EPs, holding his ground for a modern album with an old-school soul. A three-time ACM Entertainer of the Year, Aldean\u2019s a card-carrying member of Country's elite headliners whose incendiary tours are nearing legendary status. Also a dominant force on the charts, he\u2019s scored 23 Number Ones and notably all eight of Aldean\u2019s previous studio albums have been certified Gold or Platinum by the RIAA. He\u2019s the only Country act in history to top the all-genre Billboard 200 four times (in a row, no less), racking up four billion streams and more than 18 million albums sold along the way.", "otherInfo": "JASON ALDEAN: BACK IN THE SADDLE TOUR 2021"}, {"Address": "125 Hillside Rd, Chester, NJ 07930", "Artist": "Sweet Fanny Adams NJ ", "ArtistImage": "https://photos.bandsintown.com/thumb/11104629.jpeg", "Coordinates": [40.7979195, -74.70271989999999], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Sweet Fanny Adams at Bernie's Concert Field", "currentUrl": "https://www.bandsintown.com/e/102485474-sweet-fanny-adams-nj-at-bernie's-hillside-lounge?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Retro Rock & Roll Band - Original Music with interesting covers from 60's & 70's rock & roll.", "otherInfo": "Friday night outside in August - perfect"}, {"Address": "41.036062 74.639888, Sparta, NJ 07871", "Artist": "King Solomon Hicks ", "ArtistImage": "https://photos.bandsintown.com/thumb/10187836.jpeg", "Coordinates": [41.0360583, -74.63990860000001], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Blues", "Time": "7:00 PM", "Venue": "Sparta NJ Festival", "currentUrl": "https://www.bandsintown.com/e/102756903-king-solomon-hicks-at-dykstra-park-amphitheater?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "\"King\" Solomon Hicks is an American blues guitarist born in Harlem, NY. His style of music ranges from jazz, blues, classical, gospel, R&B, funk, Afro-Cuban, and classic rock. His first recording was when he was 14 with the Cotton Club All-Star Band. He has toured in Spain, Denmark, Germany, Mexico, Japan, Bahamas, Bermuda, Barbados, and the United States. His debut album, Harlem, will be released by Provogue Records in March 2020.", "otherInfo": "No event info available"}, {"Address": "40-19 Gleane St, Queens County, NY 11373", "Artist": "Jorge Glem ", "ArtistImage": "https://photos.bandsintown.com/thumb/11551218.jpeg", "Coordinates": [40.7475361, -73.882025], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "M\u00fasica Venezolana", "Time": "7:00 PM", "Venue": "Stringwise: Jorge Glem & Cesar Orozco", "currentUrl": "https://www.bandsintown.com/e/102763120-jorge-glem-at-terraza-7?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "El galardonado cuatrista, mandolinista y productor musical Jorge Glem es sin duda uno de los grandes tesoros musicales de Venezuela. Ganador del Latin Grammy, Glem es oriundo de la ciudad de Cuman\u00e1 y actualmente reside en la ciudad de Nueva York donde se ha dedicado apasionadamente a dar a conocer el cuatro venezolano, explorando junto a \u00e9l diversos ritmos de todas partes del mundo. Con su incre\u00edble talento ha logrado fusionar este instrumento tradicional de su natal Venezuela con g\u00e9neros como el jazz, la salsa, el bluegrass, el rock y el pop, compartiendo escenarios con reconocidas figuras mundiales como Paquito D'Rivera, Jordan Rudess, Rub\u00e9n Blades, Carlos Vives, Calle 13, Natalia Lafoucarde, Guaco, Desorden P\u00fablico, Ensamble Gurruf\u00edo, Gualberto Ibarreto, Etienne Charles, Gaby Moreno y muchas otras personalidades importantes en el mundo de la m\u00fasica. En 2017, Glem, en su esfuerzo continuo de internacionalizar el cuatro, inici\u00f3 un movimiento llamado #4CuatroMusic a trav\u00e9s de las redes sociales que alcanz\u00f3 m\u00e1s de mil publicaciones en solo tres semanas. Sin lugar a duda, el cuatro es m\u00e1s que un simple instrumento para este prodigio musical venezolano ya que no es solo su fiel compa\u00f1ero, sino que junto a \u00e9l ha encontrado una voz que sobrepasa cualquier frontera. Glem es tambi\u00e9n miembro fundador del aclamado grupo musical C4 Tr\u00edo, reciente ganador del Latin Grammy por el \u00e1lbum Tiempo al Tiempo junto al aclamado salsero nicarag\u00fcense, Luis Enrique. Sum\u00e1ndose a este segundo Latin Grammy, la agrupaci\u00f3n venezolana recibi\u00f3 en este 2020 su segunda nominaci\u00f3n al Grammy estadounidense. El virtuoso cuatrista ha sido galardonado con prestigiosos premios como \"El Silb\u00f3n de Oro\", 2014, \"Siembra Del Cuatro\" 2015, y tres premios Pepsi Music por su \u00e1lbum \"En El Cerrito.\" Con tres producciones propias, Glem, actualmente est\u00e1 participando en colaboraciones con el acordeonista de m\u00fasica folk Sam Reider, al igual que en otro proyecto junto al pianista, compositor y arreglista cubano-venezolano, Cesar Orozco, con quien recientemente lanz\u00f3 el \u00e1lbum, Stringwise. Glem contin\u00faa recorriendo los Estados Unidos, Am\u00e9rica Latina y Europa y espera participar en muchas m\u00e1s colaboraciones que tengan como protag\u00f3nico el ic\u00f3nico cuatro venezolano. __________________________________________________________________________ Latin Grammy-winning cuatro player, mandolinist and musical producer, Jorge Glem is without a doubt one of Venezuela\u2019s musical treasures. Born in Cuman\u00e1, Venezuela, Glem, who currently resides in New York City, has set out with a clear and driven purpose of presenting the Venezuelan cuatro as a universal instrument. His amazing talent and purpose have allowed him to share the stage with renown worldwide figures such as Paquito D\u2019Rivera, Jordan Rudess, Rub\u00e9n Blades, Carlos Vives, Calle 13, Natalia Lafoucarde, Gaby Moreno, Guaco, Desorden P\u00fablico, Ensamble Gurruf\u00edo, Gualberto Ibarreto, Etienne Charles and many other important music personalities, accomplishing the fuse of this traditional instrument in genres such as jazz, salsa, bluegrass, rock, and pop. Jorge Glem is also a founding member of the acclaimed ensemble C4Trio, who most recently won a Latin Grammy for their album \" Tiempo al Tiempo,\" with renown salsa artist Luis Enrique. Along with this second Latin Grammy win, this dynamic ensemble has been nominated three previous times for this award and is currently nominated for the second time to the prestigious Grammy presented by The Recording Academy. Jorge Glem has also been the winner of prestigious awards such as \"El Silb\u00f3n de Oro,\" \"Siembra Del Cuatro,\" and three Pepsi music awards for his album \u201cEn El Cerrito.\u201d With three productions of his own, Glem is also currently participating in collaborations with folk accordionist Sam Reider, as well as a unique duo project with Cuban-Venezuelan pianist, composer, and arranger, Cesar Orozco. In 2019, Glem and Orozco successfully released their first album together, \"Stringwise.\" Glem has participated in over 200 productions and continues touring across the U.S., Latin America, and Europe and looks forward to many more collaborations incorporating the iconic Venezuelan cuatro.", "otherInfo": "$10 suggested donation\n\nVirtuoso Venezuelan cuatro player Jorge Glem has joined forces with Cuban/Venezuelan pianist, composer and arranger C\u00e9sar Orozco, to create an unprecedented and original duo in which both performers explore their instruments to the fullest by mixing the rhythmic richness and energy of traditional Latin American genres such as Venezuelan joropo, merengue, and Cuban son with a repertoire of worldwide renowned masterpieces from pop and jazz."}, {"Address": "1186 Broadway, Brooklyn, NY 11221", "Artist": "Reliant Tom ", "ArtistImage": "https://photos.bandsintown.com/thumb/11520068.jpeg", "Coordinates": [40.6923919, -73.927463], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Avant Pop, Indie Electronic, Bedroom Pop, Art Rock, Shoegaze, Art Pop, Dreampop, Post Grunge, Rock & Pop, Indie Pop, Indie", "Time": "7:00 PM", "Venue": "Reliant Tom, Gobbinjr and Johnny Butler at Wonderville", "currentUrl": "https://www.bandsintown.com/e/102748135-reliant-tom-at-wonderville?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Reliant Tom is a Brooklyn-based indie band formed by songwriter & dancer Claire Cuny, in co-partnership with producer & sound artist Monte Weber.", "otherInfo": "Hi! We are so excited to play our next show at Wonderville in Brooklyn with 2 other amazing acts: Johnny Butler and gobbinjr. Please come and stay for all 3 acts as you won't want to miss this night!"}, {"Address": "3232 Railroad Ave, Wantagh, NY 11793", "Artist": "HalfStep ", "ArtistImage": "https://assets.prod.bandsintown.com/images/fallbackImage.png", "Coordinates": [40.67243819999999, -73.5099457], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "No genre available", "Time": "7:00 PM", "Venue": "Mulcahy's Pub and Concert Hall", "currentUrl": "https://www.bandsintown.com/e/1022476264-halfstep-at-mulcahy's-pub-and-concert-hall?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "No artist bio available", "otherInfo": "No event info available"}, {"Address": "3705 NJ-33, Neptune City, NJ 07753", "Artist": "Predator Dub Assassins ", "ArtistImage": "https://photos.bandsintown.com/thumb/8334325.jpeg", "Coordinates": [40.21144839999999, -74.091419], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Reggae", "Time": "7:00 PM", "Venue": "Predator Dub Assassins @ Delvetto's Neptune NJ", "currentUrl": "https://www.bandsintown.com/e/102709869-predator-dub-assassins-at-delvetto's-pizzeria-and-pub?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Official Website: http://dubonline.net/", "otherInfo": "Predator Dub Assassins @ Delvetto's Neptune NJ"}, {"Address": "110-00 Rockaway Blvd, Jamaica, NY 11420", "Artist": "DrDrumming ", "ArtistImage": "https://photos.bandsintown.com/thumb/9193985.jpeg", "Coordinates": [40.6727098, -73.83279569999999], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Fusion, World Fusion, Soul, Jazz Fusion, Jazz, Prog-rock, Funk, Progressive Metal, Pop, Blues, Hip Hop", "Time": "7:00 PM", "Venue": "Crash The Party at Resorts World Casino Grand Opening", "currentUrl": "https://www.bandsintown.com/e/102774717-drdrumming-at-resorts-world-casino-new-york-city?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Danny Roman is a free lance drummer based out of Brentwood, New York. He started playing at age 16 for for his high school gospel band, while growing his roots in metal back home. The two polar opposites provided a sense of versatility as well as dynamics and feel. After playing every Sunday for roughly 2 years, he has branched out and works day in and day out to be the best drummer he can be. Danny studied under professional drummer and educator Ryan Broderick, and continues to study at the Long Island Drum School. Now playing with touring cover band Crash The Party along the northeast coast, Danny is looking to leave his mark in the music world.", "otherInfo": "No event info available"}, {"Address": "77 Monmouth St, Red Bank, NJ 07701", "Artist": "Serious Matters ", "ArtistImage": "https://photos.bandsintown.com/thumb/8458598.jpeg", "Coordinates": [40.3491871, -74.0687954], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Post Hardcore, Alternative Rock", "Time": "7:00 PM", "Venue": "Serious Matters with Brave Worrier", "currentUrl": "https://www.bandsintown.com/e/102786751-serious-matters-at-red-tank-brewing-company?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "Serious Matters is a 4 piece Alternative/Post-Hardcore group from Union Beach, NJ. Formed in February of 2016 by Criss Madd and Kenny Byles of Jericho Guitars with only a few demos on hand. In September of 2016 they were joined by Matt Sandelman and began recording their debut EP, \u2018Can You See The Sky From Here\u2019 with Converse Rubber Tracks in Brooklyn, NY. The release of \u2018CYSTSFH\u2019 was published on SPIMGE (Speak Into My Good Eye) and many others. In early 2017, the band was joined by Daryian Kelton of Dark Horse Percussion and TRX Cymbals. In July of 2017, the band had signed with New York based record label, Manic Kat Records. Serious Matters released a new EP titled, \u2018Through It All\u2019, mixed and mastered by John Naclerio of Nada Studios on January 5th, 2018.", "otherInfo": "No event info available"}, {"Address": "64 1/2 Chestnut St, Montclair, NJ 07042", "Artist": "Max Kaplan & The Magics ", "ArtistImage": "https://photos.bandsintown.com/thumb/11494333.jpeg", "Coordinates": [40.821022, -74.20973099999999], "Date": "2021-08-06", "EventDate": "Aug. 6th, 2021", "Genre": "Classic Rock, Rock, Pop, Blues, Soul, Classic Blues, Retro-soul", "Time": "7:00 PM", "Venue": "Ruthie's Bar-B-Q & Pizza", "currentUrl": "https://www.bandsintown.com/e/102733216-max-kaplan-and-the-magics-at-ruthie's-bar-b-q-and-pizza?came_from=257&utm_medium=web&utm_source=home&utm_campaign=event", "moreBioInfo": "With a captivating front-man, soulful guitar leads, and a tight rhythm section, the Memphis-based artist Max Kaplan has an exciting and original sound harkening back to the sonic landscape of funky Motown and Stax era soul, to the guitar driven blues of the 1960\u2019s. Max is an energetic vocalist and stage performer, resulting in a poignant display of musical performance through the medium of Memphis soul songwriting. His influences include B.B. King, Leon Bridges, James Brown, Alabama Shakes, Al Green, The Staples Singers etc.", "otherInfo": "No event info available"}, {"Address": "198 Broadway, Amityville, NY 11701", "Artist": "Somerset Thrower ", "ArtistImage": "https://photos.bandsintown.com/thumb/6449719.jpeg", "Coordinates": [40.6789326, -73.4176297], "Date": "2021-08-06", "EventDate": "Aug. 6